<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.materialdesignstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SupportUseActivity" />
        <activity android:name=".activity.RecycleViewActivity" />
        <activity android:name=".activity.ChangeRecycleViewActivity" />
        <activity android:name=".activity.WrapRecycleViewActivity" />
        <activity android:name=".itemtouch.ItemTouchActivity"></activity>
    </application>

</manifest>